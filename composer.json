{
  "name": "tourze/easy-admin-attribute",
  "description": "EasyAdminBundle Attribute Extend",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "chrisullyott/php-filesize": "^4.2"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\EasyAdmin\\Attribute\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\EasyAdmin\\Attribute\\Tests\\": "tests"
    }
  },
  "scripts": {
    "phpstan": "phpstan analyse src -l 1",
    "test": "phpunit"
  }
}